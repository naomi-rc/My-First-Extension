{
    "manifest_version": 2,
    "name": "My First Extension",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.5.1.js", "content.js"]
        }
    ],
    "browser_action":{
        "default_icon": "code.png"
    }
}